g++ -g -Iinclude -Iinclude/detect -Iinclude/heap -Iinclude/util -Iinclude/sync -DDETECT_FALSE_SHARING -msse3 -DSSE_SUPPORT -fno-omit-frame-pointer -m32 -DX86_32BIT  -I. -I./heaplayers -I./heaplayers/util -shared -fPIC -D'CUSTOM_PREFIX(x)=sheriff_##x'  source/libsheriff.cpp source/realfuncs.cpp source/xthread.cpp source/dlmalloc.c source/finetime.c source/gnuwrapper.cpp -o libsheriff_detect32.so  -ldl -lpthread
g++ -g -Iinclude -Iinclude/detect -Iinclude/heap -Iinclude/util -Iinclude/sync -DDETECT_FALSE_SHARING -msse3 -DSSE_SUPPORT -fno-omit-frame-pointer -m64  -I. -I./heaplayers -I./heaplayers/util -shared -fPIC -D'CUSTOM_PREFIX(x)=sheriff_##x'  source/libsheriff.cpp source/realfuncs.cpp source/xthread.cpp source/dlmalloc.c source/finetime.c source/gnuwrapper.cpp -o libsheriff_detect64.so  -ldl -lpthread

